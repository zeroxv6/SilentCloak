spring.application.name=SilentCloak

spring.datasource.url=jdbc:postgresql://localhost:5432/silentcloak
spring.datasource.username=postgres
spring.datasource.password=PASSWORD

spring.jpa.hibernate.ddl-auto=update

spring.jpa.show-sql=false
spring.jpa.properties.hibernate.format_sql=true

jwt.secret=SECRET
jwt.expiration=86400000

app.security.keystore-path=identity.p12
app.security.keystore-password=PASSWORD
app.security.key-alias=server-identity
